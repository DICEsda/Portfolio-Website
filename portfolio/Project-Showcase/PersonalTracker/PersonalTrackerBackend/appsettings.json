{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GoogleCalendar": {
    "ClientId": "YOUR_GOOGLE_CLIENT_ID",
    "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET",
    "ApplicationName": "PersonalTracker"
  },
  "GoogleBooks": {
    "ApiKey": "YOUR_GOOGLE_BOOKS_API_KEY"
  },
  "GoogleAuth": {
    "ClientId": "YOUR_GOOGLE_OAUTH_CLIENT_ID"
  },
  "Jwt": {
    "SecretKey": "YourSecretKeyHereAtLeast256Bits",
    "Issuer": "PersonalTracker",
    "Audience": "PersonalTracker"
  },
  "OpenAI": {
    "ApiKey": "YOUR_OPENAI_API_KEY"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Data/personaltracker.db"
  }
}
